[dotnet]

project/assembly_name="avenger-sharks"

[editor]

movie_writer/movie_file="/Users/greavet/src/Godot/movies/test.avi"
movie_writer/fps=30
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

